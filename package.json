{
  "name": "openperseus",
  "version": "1.2.0",
  "description": "Open-source community fork of Khan Academy's Perseus questions/exercises/articles framework",
  "keywords": [
    "khan",
    "exercises",
    "perseus",
    "interactive",
    "questions",
    "education"
  ],
  "author": "Aria Buckles <aria@buckles.dev> (https://github.com/ariabuckles)",
