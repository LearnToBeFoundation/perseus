      "last 2 firefox versions",
      "last 2 safari versions",
      "last 2 edge versions",
      "Firefox ESR",
      "maintained node versions",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
