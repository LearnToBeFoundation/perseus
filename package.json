  ],
  "license": "MIT",
  "main": "perseus.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "rollup -c && react-scripts build",
    "postbuild": "rm -rf ./docs/; mv ./build/ ./docs",
    "prepare": "rollup -c",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": []
  },
  "browserslist": {
